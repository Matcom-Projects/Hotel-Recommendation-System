    "# Create a text classification pipeline using the specified model and tokenizer\n",
    "# `device=-1` indicates that the CPU is used for inference; set to `device=0` if a GPU is available\n",
    "pipeline = TextClassificationPipeline(model=model, tokenizer=tokenizer, device=-1)  # Use device=0 if a GPU is available"
   ]
