   "outputs": [],
   "source": [
    "# Load the CSV file into a DataFrame\n",
    "df = pd.read_csv(redirect('Datasets', 'Datafiniti_Hotel_Reviews_Jun19.csv'))\n",
    "\n",
    "# Define the columns of interest to keep in the DataFrame\n",
    "columns_of_interest = [\"address\", \"categories\", \"city\", \"country\", \"name\", \"province\", \"reviews.rating\", \"reviews.text\"]\n",
    "\n",
    "# Select only the specified columns from the DataFrame\n",
    "df = df[columns_of_interest]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Clean the text data in the 'reviews.text' column\n",
    "# 1. Convert all text to lowercase and ensure it is treated as a string.\n",
    "df[\"reviews.text\"] = df[\"reviews.text\"].apply(lambda x: str(x).lower() if isinstance(x, str) else str(x))\n",
    "\n",
    "# 2. Apply a series of text cleaning operations:\n",
    "#   -Remove emojis from the text.\n",
    "#   -Remove punctuation marks from the text.\n",
    "#   -Remove numbers from the text.\n",
    "#   -Perform any additional cleaning (e.g., whitespace trimming or normalization) on the cleaned text.\n",
    "df[\"reviews.text\"] = df[\"reviews.text\"].apply(lambda x: modulo.remove_numbers(modulo.remove_punct(clean(modulo.remove_emoji(x))[0][0])))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define a mapping of label names to numerical values\n",
    "# This mapping converts text labels into corresponding numerical sentiment scores.\n",
    "label_mapping = {\n",
    "    \"LABEL_0\": -0.5,  # Example: \"LABEL_0\" corresponds to a sentiment score of -0.5\n",
    "    \"LABEL_1\": 1.0,   # Example: \"LABEL_1\" corresponds to a sentiment score of 1.0\n",
    "    \"LABEL_2\": 2.5    # Example: \"LABEL_2\" corresponds to a sentiment score of 2.5\n",
    "}\n",
    "\n",
    "def process_text(text):\n",
    "    # Classify the input text using the text classification pipeline\n",
    "    # The 'truncation=True' ensures that text longer than the maximum length is truncated,\n",
    "    # and 'max_length=512' sets the maximum input length for the model.\n",
    "    prediction = pipeline(text, truncation=True, max_length=512)\n",
    "    \n",
    "    # Extract the predicted label and confidence score from the prediction result\n",
    "    label = prediction[0]['label']\n",
    "    confidence = prediction[0]['score']\n",
    "    \n",
    "    # Map the label to its corresponding sentiment score using the label_mapping dictionary.\n",
    "    # If the label is not found in the dictionary, it returns the label as-is.\n",
    "    sentiment = label_mapping.get(label, label)\n",
    "    \n",
    "    # Compute the text score by multiplying the sentiment score by the confidence score\n",
    "    text_score = sentiment * confidence\n",
    "    \n",
    "    # Return the sentiment score, confidence score, and text score\n",
    "    return sentiment, confidence, text_score"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Apply the 'process_text' function to each row in the 'reviews.text' column\n",
    "# The 'process_text' function performs text classification, returning sentiment, confidence, and text score.\n",
    "# The lambda function applies 'process_text' to each element in 'reviews.text', and the results are expanded into\n",
    "# three separate columns: 'sentiment', 'confidence', and 'text_score'.\n",
    "\n",
    "df[['sentiment', 'confidence','text_score']] = df['reviews.text'].apply(lambda x: pd.Series(process_text(x)))                                               "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate the final score by combining the review rating and text score.\n",
    "# The final score is computed as follows:\n",
    "# - Multiply the review rating by 1.5\n",
    "# - Add the text score obtained from the text classification process\n",
    "# The result is stored in a new column called 'final_score'.\n",
    "\n",
    "df['final_score'] = df['reviews.rating'] * 1.5 + df['text_score']"
   ]
  },
